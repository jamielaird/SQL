/*Number of Effective Calls by Student*/

SELECT
  CC.client_id,
  CC.crms_number,
  COUNT(CC.id) [NumEffectiveCalls]
FROM dbo.ZZ_crms_communication CC
WHERE CC.in_out = 'OUT'
AND CC.class = 'PHONE'
AND CC.call_effectiveness = 'Effective'

GROUP BY CC.client_id,
         CC.crms_number

ORDER BY CC.client_id ASC, CC.crms_number ASC
